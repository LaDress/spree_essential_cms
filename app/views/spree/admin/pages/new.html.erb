<%= render :partial => 'spree/admin/shared/page_tabs', :locals => {:current => "Page Details"} %>
<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @page } %>

<%= form_for([:admin, @page], :html => { :multipart => true }) do |f| %>
  <fieldset>
    <legend><%= t('.new_page') %></legend>
    <%= render "form", :form => f %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
