<%= render :partial => 'spree/admin/shared/page_tabs', :locals => {:current => 'Contents'} %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @content } %>

<%= form_for([:admin, @page, @content], :html => { :multipart => true }) do |f| %>
  <fieldset>
    <legend><%= t('.new_content') %></legend>
    <%= render "form", :form => f %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
